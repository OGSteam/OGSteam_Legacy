QGroupBox {
	font-size: 11px;
	font-weight: bold;
}

QGroupBox::title {
	subcontrol-position: top center;
}

QLabel {
	font-size: 11px;
}

QLineEdit {
	font-size: 11px;
}

QDateTimeEdit {
	font-size: 11px;
}

#lPt, #lGt, #lCle, #lClo, #lCr, #lVb, #lVc, #lRe, #lSe, #lSs, #lBo, #lDe, #lEm, #lTr, #lLm, #lLe, #lLo, #lCg, #lAi, #lLp, #lPb, #lGb, #lMi {
	font-size: 9px;
}

#labelPertes, #labelPertesAmin, #labelPertesAmoy, #labelPertesAmax, #labelPertesDmoy, #labelPertesDmax, #labelPertesDmin {color: #aa0000}
#labelCdr, #buttinT, #labelCdrMin, #labelCdrMoy, #labelCdrMax {color: #000088}
#labelGains, #buttinR, #labelTotal, #labelGainsMin, #labelGainsMoy, #labelGainsMax {color: #006600}

QLCDNumber {
	color: green;
	background: qlineargradient(x1:0, y1:1, x2:0, y2:0, stop:0 black, stop: 0.5 #113311, stop:1 black);
}



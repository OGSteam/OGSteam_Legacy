<bbcode>
	<tag name="tab" close="no">
		<begin><![CDATA[&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;]]></begin>
		<finish><![CDATA[]]></finish>
	</tag>
	<tag name="color" param="yes">
		<begin><![CDATA[<span style="color:{PARAM}">]]></begin>
		<finish><![CDATA[</span>]]></finish>
	</tag>
	<tag name="size" param="yes">
		<begin><![CDATA[<span style="font-size:{PARAM}px">]]></begin>
		<finish><![CDATA[</span>]]></finish>
	</tag>
	<tag name="b">
		<begin><![CDATA[<span style="font-weight:bold">]]></begin>
		<finish><![CDATA[</span>]]></finish>
	</tag>
	<tag name="i">
		<begin><![CDATA[<span style="font-style:italic" value="{VALUE}" param="{PARAM}">]]></begin>
		<finish><![CDATA[</span>]]></finish>
	</tag>
	<tag name="u">
		<begin><![CDATA[<span style="text-decoration:underline">]]></begin>
		<finish><![CDATA[</span>]]></finish>
	</tag>
	<tag name="nobbcode" render="no">
		<begin></begin>
		<finish></finish>
	</tag>
	<tag name="nohtml" function="htmlspecialchars">
		<begin></begin>
		<finish></finish>
	</tag>
	<tag name="url" param="yes">
		<begin><![CDATA[<a href="{PARAM}" target="_blank">]]></begin>
		<finish><![CDATA[</a>]]></finish>
	</tag>
	<tag name="url" param="no">
		<begin><![CDATA[<a href="{VALUE}" target="_blank">]]></begin>
		<finish><![CDATA[</a>]]></finish>
	</tag>
	<tag name="img" keepvalue="no">
		<begin><![CDATA[<img src="{VALUE}" border="0" />]]></begin>
		<finish><![CDATA[]]></finish>
	</tag>
	<tag name="img" keepvalue="no" param="yes">
		<begin><![CDATA[<img src="{VALUE}" border="0" align="{PARAM}"/>]]></begin>
		<finish><![CDATA[ ]]></finish>
	</tag>
	<tag name="*" close="no" parent="list">
		<begin><![CDATA[<li>]]></begin>
		<finish><![CDATA[]]></finish>
	</tag>
	<tag name="list">
		<begin><![CDATA[<ul>]]></begin>
		<finish><![CDATA[</ul>]]></finish>
	</tag>
	<tag name="list" param="yes">
		<begin><![CDATA[<ol type="{PARAM}">]]></begin>
		<finish><![CDATA[</ol>]]></finish>
	</tag>
	<tag name="quote">
		<begin><![CDATA[<table width="95%" cellpadding="5" border="1" align="center"><tr><td bgcolor="#FAFAFA">]]></begin>
		<finish><![CDATA[</td></tr></table>]]></finish>
	</tag>
	<tag name="align" param="yes">
		<begin><![CDATA[<div align="{PARAM}">]]></begin>
		<finish><![CDATA[</div>]]></finish>
	</tag>
	<tag name="right">
		<begin><![CDATA[<div align="right">]]></begin>
		<finish><![CDATA[</div>]]></finish>
	</tag>
	<tag name="left">
		<begin><![CDATA[<div align="left">]]></begin>
		<finish><![CDATA[</div>]]></finish>
	</tag>
	<tag name="center">
		<begin><![CDATA[<div align="center">]]></begin>
		<finish><![CDATA[</div>]]></finish>
	</tag>
	<tag name="blink">
		<begin><![CDATA[<blink>]]></begin>
		<finish><![CDATA[</blink>]]></finish>
	</tag>		
	
</bbcode>